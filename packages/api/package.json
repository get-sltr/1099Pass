{
  "name": "@1099pass/api",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@1099pass/shared": "*",
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/client-kms": "^3.500.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/client-secrets-manager": "^3.500.0",
    "@aws-sdk/client-ses": "^3.500.0",
    "@aws-sdk/client-sns": "^3.500.0",
    "@aws-sdk/client-sqs": "^3.500.0",
    "@aws-sdk/lib-dynamodb": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.0",
    "plaid": "^23.0.0",
    "stripe": "^14.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/services/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
